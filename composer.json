{
  "name": "mackey/yandex",
  "description": "PHP SDK для работы с некоторыми сервисами яндекса (Яндекс.Диск, Yandex.Disk)",
  "minimum-stability": "dev",
  "keywords": ["yandex", "disk", "диск", "яндекс"],
  "license": "MIT",
  "authors": [
    {
      "name": "Dmitry Arhitector",
      "email": "email@example.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "zendframework/zend-diactoros": "1.3.3",
    "arhitector/http-curl-client": ">=1.2",
    "league/event": "2.1.2",
    "php-http/message": "dev-master",
    "php-http/plugins": "v1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "5.2.*"
  },
  "autoload": {
    "Mackey\\Yandex\\": "src/"
  },
  "autoload-dev": {
    "psr-4": {
      "Mackey\\Yandex\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
